<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leaf_diseases_app">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sgicono"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sgicono"
        android:supportsRtl="true"
        android:theme="@style/Theme.Leaf_diseases_app">
        <activity android:name=".Scab"></activity>
        <activity android:name=".Rust" />
        <activity android:name=".Powdery_mildew" />
        <activity android:name=".Healthy" />
        <activity android:name=".Frog_eye_leaf_spot" />
        <activity android:name=".complex" />
        <activity android:name=".Diseases" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>